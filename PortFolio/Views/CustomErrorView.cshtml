@model CustomError
@{
    Layout = "";
}

<!DOCTYPE html>
<html lang="en">

<!-- Mirrored from coderthemes.com/adminto/layouts/pages-500.html by HTTrack Website Copier/3.x [XR&CO'2014], Fri, 09 Sep 2022 12:35:09 GMT -->
<head>
    <meta charset="utf-8" />
    <title>Page d'erreur</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta content="A fully featured admin theme which can be used to build CRM, CMS, etc." name="description" />
    <meta content="Coderthemes" name="author" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <!-- App favicon -->
    <link rel="shortcut icon" href="~/admin/images/favicon.ico">

    <!-- App css -->

    <link href="~/admin/css/app.min.css" rel="stylesheet" type="text/css" id="app-style" />

    <!-- icons -->
    <link href="~/admin/css/icons.min.css" rel="stylesheet" type="text/css" />

</head>

<body class="loading authentication-bg authentication-bg-pattern">

    <div class="account-pages mt-5 mb-5">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-md-8 col-lg-6 col-xl-5">
                    <div class="text-center">
                        <a href="/" class="logo">
                            <img src="~/admin/images/logo-dark.png" alt="" height="22" class="logo-light mx-auto">
                        </a>
                        <p class="text-muted mt-2 mb-4">Erreur</p>
                    </div>
                    <div class="card">

                        <div class="card-body p-4">

                            <div class="text-center">
                                <h1 class="text-error">@Model.StatusCode</h1>
                                <h3 class="mt-3 mb-2">Erreur</h3>
                                <p class="text-muted mb-3">@Model.ErrorMessage <a href="#" class="text-dark"></a></p>

                                <a href="/" class="btn btn-danger waves-effect waves-light"><i class="fas fa-home me-1"></i> Retour</a>
                            </div>


                        </div> <!-- end card-body -->
                    </div>
                    <!-- end card -->

                </div> <!-- end col -->
            </div>
            <!-- end row -->
        </div>
        <!-- end container -->
    </div>
    <!-- end page -->
    <!-- Vendor -->
    <script src="~/admin/libs/jquery/jquery.min.js"></script>
    <script src="~/admin/libs/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/admin/libs/simplebar/simplebar.min.js"></script>
    <script src="~/admin/libs/node-waves/waves.min.js"></script>
    <script src="~/admin/libs/waypoints/lib/jquery.waypoints.min.js"></script>
    <script src="~/admin/libs/jquery.counterup/jquery.counterup.min.js"></script>
    <script src="~/admin/libs/feather-icons/feather.min.js"></script>

    <!-- App js -->
    <script src="~/admin/js/app.min.js"></script>

</body>

<!-- Mirrored from coderthemes.com/adminto/layouts/pages-500.html by HTTrack Website Copier/3.x [XR&CO'2014], Fri, 09 Sep 2022 12:35:09 GMT -->
</html>