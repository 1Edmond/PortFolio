<!DOCTYPE html>
<html lang="fr">

<!-- Mirrored from coderthemes.com/adminto/layouts/index.html by HTTrack Website Copier/3.x [XR&CO'2014], Fri, 09 Sep 2022 12:33:16 GMT -->
<head>

    <meta charset="utf-8" />
    <title>Dashboard | Adminto - Responsive Admin Dashboard Template</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta content="A fully featured admin theme which can be used to build CRM, CMS, etc." name="description" />
    <meta content="Coderthemes" name="author" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <!-- App favicon -->
    <link rel="shortcut icon" href="~/admin/images/favicon.ico">

    <!-- App css -->
    <link href="~/admin/css/app.min.css" rel="stylesheet" type="text/css" id="app-style" />

    <!-- icons -->
    <link href="~/admin/css/icons.min.css" rel="stylesheet" type="text/css" />
    <link href="~/DataTables/datatables.css" rel="stylesheet" type="text/css" />
    <link href="~/notification/colored-theme.min.css" rel="stylesheet" type="text/css" />

    @RenderSection("Styles",false)
   
</head>

<!-- body start -->
<body class="loading" data-layout-color="light" data-layout-mode="default" data-layout-size="fluid" data-topbar-color="light" data-leftbar-position="fixed" data-leftbar-color="light" data-leftbar-size='default' data-sidebar-user='true'>

    <!-- Begin page -->
    <div id="wrapper">
        <!-- Topbar Start -->
        <partial name="_TopBar" />
        <!-- end Topbar -->
        <!-- ========== Left Sidebar Start ========== -->

        <partial name="_LeftSideBar" />
        <!-- Left Sidebar End -->
        <!-- ============================================================== -->
        <!-- Start Page Content here -->
        <!-- ============================================================== -->

        <div class="content-page">
            <!-- content -->
            <div class="content">

            @RenderSection("Content")
            <!-- Footer Start -->
            </div>
            <footer class="footer">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-6">
                            <script>document.write(new Date().getFullYear())</script> &copy; Adminto theme by <a href="#">Coderthemes</a>
                        </div>
                        <div class="col-md-6">
                            <div class="text-md-end footer-links d-none d-sm-block">
                                <a href="javascript:void(0);">About Us</a>
                                <a href="javascript:void(0);">Help</a>
                                <a href="javascript:void(0);">Contact Us</a>
                            </div>
                        </div>
                    </div>
                </div>
            </footer>
            <!-- end Footer -->

        </div>
        <!-- ============================================================== -->
        <!-- End Page content -->
        <!-- ============================================================== -->


    </div>
    <!-- END wrapper -->
    <!-- Right Sidebar -->
    <partial name="_RightSideBar" />
    <!-- /Right-bar -->
    <!-- Right bar overlay-->
    <div class="rightbar-overlay"></div>

    <!-- Vendor -->
    <script src="~/admin/libs/jquery/jquery.min.js"></script>
    <script src="~/admin/libs/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/admin/libs/simplebar/simplebar.min.js"></script>
    <script src="~/admin/libs/node-waves/waves.min.js"></script>
    <script src="~/admin/libs/waypoints/lib/jquery.waypoints.min.js"></script>
    <script src="~/admin/libs/jquery.counterup/jquery.counterup.min.js"></script>
    <script src="~/admin/libs/feather-icons/feather.min.js"></script>

    <!-- knob plugin -->
    <script src="~/admin/libs/jquery-knob/jquery.knob.min.js"></script>

    <!--Morris Chart-->
    <script src="~/admin/libs/morris.js06/morris.min.js"></script>
    <script src="~/admin/libs/raphael/raphael.min.js"></script>

    <!-- Dashboar init js-->
    <script src="~/admin/js/pages/dashboard.init.js"></script>

    <!-- App js-->
    <script src="~/admin/js/app.min.js"></script>
    
    @RenderSection("Scripts", false)
    <script src="~/notification/growl-notification.min.js"></script>
    <script src="~/DataTables/datatables.min.js"></script>

    @{
        if (ViewBag.Notif != null)
        {
            var notification = ViewBag.Notif as MyNotification;
            if(notification  != null){
                    <script>
                        GrowlNotification.notify({
                            title: "@notification.Libelle",
                            description: "@notification.Description",
                            type: "@notification.Type",
                            position: 'top-center',
                            closeTimeout: @Constants.AnnimationDuration
                        })
                    </script>
            }
        }
    }
</body>

